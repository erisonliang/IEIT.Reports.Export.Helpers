<?xml version="1.0"?>
<package >
  <metadata>
    <id>IEIT.Reports.Export.Helpers</id>
	<version>0.3.8</version>
	<title>Excel Helpers</title>
    <authors>sultan.imbayev, yerlanyr</authors>
    <owners>IPEIT KBTU</owners>
    <projectUrl>https://github.com/sultanimbayev/IEIT.Reports.Export.Helpers</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
        Shortcut extensions for DocumentFormat.OpenXml.Spreadsheet classes
        Расширения-шорткоды для классов из библиотеки DocumentFormat.OpenXml.Spreadsheet
	</description>
    <summary>
        Даннный пакет предназначен для упрощения синтаксиса библиотеки DocumentFormat.OpenXml.
		This package is to simplify work with Excel Spreadsheet document sing DocumentFormat.OpenXml library.
    </summary>
    <releaseNotes>
		Shape SetWidthInPixels, SetHeightInPixels extensions major fix
		Picture SetWidthInPixels, SetHeightInPixels extensions major fix
	</releaseNotes>
    <tags>OpenXml Excel</tags>
	<dependencies>
		<dependency id="DocumentFormat.OpenXml" version="2.5.0"/>
	</dependencies>
	<frameworkAssemblies>
	  <frameworkAssembly assemblyName="System.Configuration" />
	</frameworkAssemblies>
  </metadata>
  <files>
	<file src="..\IEIT.Reports.Export.Helpers\bin\Release\IEIT.Reports.Export.Helpers.dll" target="lib\net45" />
	<file src="..\IEIT.Reports.Export.Helpers\bin\Release\IEIT.Reports.Export.Helpers.xml" target="lib\net45" />
	<file src="..\IEIT.Reports.Export.Helpers\content\**" target="content" />
  </files>
</package>